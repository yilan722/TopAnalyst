    "dotenv": "^17.2.2",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.292.0",
    "next": "^14.0.0",
    "node-fetch": "^3.3.2",
    "pdf-parse": "^1.1.1",
    "pdf-poppler": "^0.2.3",
    "pdf2pic": "^3.2.0",
    "pdfjs-dist": "^5.4.149",
    "postcss": "^8.4.0",
