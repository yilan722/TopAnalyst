{
  "name": "superanalystpro",
  "version": "1.0.0",
  "description": "Professional AI-powered stock analysis platform - SuperAnalystPro",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:backend": "cd backend && python run.py",
    "dev:celery": "cd backend && celery -A celery_worker worker --loglevel=info",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:backend\" \"npm run dev:celery\"",
    "build": "next build",
    "build:cloudflare": "node build-cloudflare.js",
    "start": "next start",
    "lint": "next lint",
    "format:test": "node scripts/test-report-format.js",
    "format:lock": "node scripts/format-lock.js lock",
    "format:check": "node scripts/format-lock.js check",
    "format:monitor": "node scripts/format-monitor.js monitor",
    "format:scan": "node scripts/format-monitor.js scan",
    "format:consistency": "node scripts/test-format-consistency.js",
    "update-report": "node scripts/update-todays-report.js",
    "backend:install": "cd backend && pip install -r requirements.txt",
    "backend:dev": "cd backend && python run.py",
    "backend:celery": "cd backend && celery -A celery_worker worker --loglevel=info",
    "backend:flower": "cd backend && celery -A celery_worker flower"
  },
